{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        }
    },
    "ScheduleOptions": {
        "Cron": "* * * * *",
        "ErrorDelayTimeSec": 5
    },
    "ForecastSourceOptions": {
        "BaseUri": "https://www.gismeteo.ru/"
    },
    "GrpcOptions": {
        "Address": "http://localhost:5126"
    }
}